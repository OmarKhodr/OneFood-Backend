{
  "name": "onefood-backend",
  "version": "1.0.0",
  "description": "Backend NodeJS server for OneFood",
  "scripts": {
    "dev": "tsx --watch --env-file .env.development src/index.ts",
    "dev:staging": "tsx --watch --env-file .env.staging src/index.ts",
    "dev:prod": "tsx --watch --env-file .env.production src/index.ts",
    "start": "node --env-file .env dist/index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OmarKhodr/OneFood-Backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/OmarKhodr/OneFood-Backend/issues"
  },
  "homepage": "https://github.com/OmarKhodr/OneFood-Backend#readme",
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tsconfig/node24": "^24.0.3",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  }
}
